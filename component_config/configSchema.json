{
  "type": "object",
  "title": "ServiceNow table extractor configuration",
  "required": [
    "user",
    "#password"
  ],
  "properties": {
    "user": {
      "type": "string",
      "title": "Username",
      "propertyOrder": 1
    },
    "#password": {
      "type": "string",
      "title": "Password",
      "format": "password",
      "propertyOrder": 2
    },
    "server": {
      "type": "string",
      "title": "Server",
      "description": "server url",
      "propertyOrder": 3
    },
    "threads": {
      "type": "integer",
      "title": "Threads",
      "default": 8,
      "description": "optional - number of threads to use to fetch data",
      "propertyOrder": 4
    },
    "output_bucket": {
      "type": "string",
      "title": "Output bucket",
      "default": "kds-team.ex-servicenow-tables",
      "description": "The name of the bucket that results will be stored to.",
      "propertyOrder": 5
    }
  }
}